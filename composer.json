{
    "name": "tocaan/laravel-translation-manager",
    "description": "Enhanced Laravel Translation Manager",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "homepage": "https://github.com/Tocaanco/new-laravel-translation-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Tocaanco",
            "email": "dev@gtocaan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^7.0",
        "laravelcollective/html": "^6.2",
        "illuminate/translation": "^7.2",
        "doctrine/dbal": "^2.10",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~6.1",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "classmap": [
            "src/Classes"
        ],
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Vsch\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "stable"
}
